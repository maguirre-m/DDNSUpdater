{
  "IPAddressSavePath": "LastKnownIPAddress.txt",
  "SqlServerDataStore": {
    "ConnectionString": "Data Source=localhost;Integrated Security=True;Initial Catalog=DevSecrets;TrustServerCertificate=True"
  },
  "CloudFlare": {
    "RecordName" : "gatekeeper.dreamspire.dev"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Updater.log", "rollingInterval": "Day" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "DDNS Updater"
    }
  }
}